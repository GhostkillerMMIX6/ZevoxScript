--Mod Scripts

SpeedBoost.MouseButton1Click:Connect(function()
	game.StarterGui:SetCore("SendNotification", {
		Title = "Dash is loaded",
		Text = "Press E to Dash. Have fun beeing faster then others",
		Duration = 5
	})
	local UserInputService = game:GetService("UserInputService")
	local player = game.Players.LocalPlayer
	local character = player.Character or player.CharacterAdded:Wait()
	local humanoid = character:WaitForChild("Humanoid")
	local runService = game:GetService("RunService")

	local dashSpeed = 20 -- Geschwindigkeit beim Dashen
	local dashDuration = 0.2 -- Dauer des Dashs in Sekunden
	local isDashing = false
	local dashActive = true -- Variable, um das Script aktiv zu halten

	local function dash()
		if isDashing then return end -- Verhindert mehrfaches Dashen gleichzeitig
		isDashing = true

		local rootPart = character:WaitForChild("HumanoidRootPart")
		local originalWalkSpeed = humanoid.WalkSpeed

		-- Richtung des Spielers ermitteln
		local lookVector = rootPart.CFrame.LookVector

		-- Geschwindigkeit temporär erhöhen
		humanoid.WalkSpeed = dashSpeed

		-- Dash für die Dauer
		local startTime = tick()
		while tick() - startTime < dashDuration do
			if not dashActive then
				break -- Beendet den Dash, wenn dashActive false ist
			end
			rootPart.CFrame = rootPart.CFrame + lookVector * dashSpeed * runService.Heartbeat:Wait()
		end

		-- Zurücksetzen der Geschwindigkeit
		humanoid.WalkSpeed = originalWalkSpeed
		isDashing = false
	end

	-- Überwacht Tastendrücke
	UserInputService.InputBegan:Connect(function(input, gameProcessed)
		if gameProcessed then return end

		if input.KeyCode == Enum.KeyCode.E then
			dash()
		elseif input.KeyCode == Enum.KeyCode.Q then
			dashActive = false -- Script deaktivieren, z.B. beim Drücken von Q
		end
	end)
end)


SpeedBoostOff.MouseButton1Click:Connect(function()
	game.StarterGui:SetCore("SendNotification", {
		Title = "Sorry",
		Text = "You can not turn it off yet",
		Duration = 3
	})
	local UserInputService = game:GetService("UserInputService")
	local player = game.Players.LocalPlayer
	local character = player.Character or player.CharacterAdded:Wait()
	local humanoid = character:WaitForChild("Humanoid")
	local runService = game:GetService("RunService")

	local dashSpeed = 0 -- Geschwindigkeit beim Dashen
	local dashDuration = 0 -- Dauer des Dashs in Sekunden
	local isDashing = false

	local function dash()
		if isDashing then return end -- Verhindert mehrfaches Dashen gleichzeitig
		isDashing = true

		local rootPart = character:WaitForChild("HumanoidRootPart")
		local originalWalkSpeed = humanoid.WalkSpeed

		-- Richtung des Spielers ermitteln
		local lookVector = rootPart.CFrame.LookVector

		-- Geschwindigkeit temporär erhöhen
		humanoid.WalkSpeed = dashSpeed

		-- Dash für die Dauer
		local startTime = tick()
		while tick() - startTime < dashDuration do
			rootPart.CFrame = rootPart.CFrame + lookVector * dashSpeed * runService.Heartbeat:Wait()
		end

		-- Zurücksetzen der Geschwindigkeit
		humanoid.WalkSpeed = originalWalkSpeed
		isDashing = false
	end

	-- Dashen bei Drücken der Taste "E"
	UserInputService.InputBegan:Connect(function(input, gameProcessed)
		if gameProcessed then return end
		if input.KeyCode == Enum.KeyCode.E then
			dash()
		end
	end)
end)

